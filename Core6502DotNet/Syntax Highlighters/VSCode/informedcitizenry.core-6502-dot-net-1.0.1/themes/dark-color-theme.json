{
  "$schema": "vscode://schemas/color-theme",
  "name": "Core 6502.Net Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#d4d4d4",
    "editor.selectionBackground": "#7e7b7b",
    "editor.lineHighlightBackground": "#505050"
  },
  "tokenColors": [
    {
      "name": "Block Comment",
      "scope": "comment.block",
      "settings": {
        "foreground": "#e3bcbc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Line Comment",
      "scope": "comment.line",
      "settings": {
        "foreground": "#78a093",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#ce9c89"
      }
    },
    {
      "scope": "keyword.control.directive",
      "settings": {
        "foreground": "#91c1e9",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.other.mnemonic",
      "settings": {
        "foreground": "#7e9fe9",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.control.pseudoop",
      "settings": {
        "foreground": "#ce9c89",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.define",
      "settings": {
        "foreground": "#da8072"
      }
    },
    {
      "scope": "keyword.register",
      "settings": {
        "foreground": "#9dcca7",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#aaa58d"
      }
    },
    {
      "scope": "keyword.function",
      "settings": {
        "foreground": "#8bb8dd"
      }
    },
    {
      "scope": "keyword.other.macro",
      "settings": {
        "foreground": "#b5cea8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#e9a4a4"
      }
    }
  ]
}