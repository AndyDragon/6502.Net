<NotepadPlus>
    <UserLang name="Sixty502DotNeti80" ext="i80" udlVersion="3.0">
        <Settings>
            <Global caseIgnored="yes" allowFoldOfComments="yes" foldCompact="no" forcePureLC="0" decimalSeparator="0" />
