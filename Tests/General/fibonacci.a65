fibonacci    .function num
                .if num < 2
                    .return num
                .endif
                .return fibonacci(num - 1) + fibonacci(num - 2)
            .endfunction
            .byte fibonacci(0), fibonacci(1), fibonacci(2), fibonacci(3),
                  fibonacci(4), fibonacci(5), fibonacci(6), fibonacci(7)